                <property name="receives_default">False</property>
                <property name="use_action_appearance">False</property>
                <property name="xalign">0.5</property>
                <property name="draw_indicator">False</property>
              </object>
              <packing>
                <property name="left_attach">1</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkRadioButton" id="btnMiddleRight">
                <property name="label" translatable="yes">MR</property>
                <property name="use_action_appearance">False</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">False</property>
                <property name="use_action_appearance">False</property>
                <property name="xalign">0.5</property>
                <property name="draw_indicator">False</property>
                <property name="group">btnHide</property>
              </object>
              <packing>
                <property name="left_attach">2</property>
                <property name="top_attach">1</property>
              </packing>
            </child>
            <child>
              <object class="GtkRadioButton" id="btnBottomLeft">
                <property name="label" translatable="yes">BL</property>
