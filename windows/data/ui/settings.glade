          </packing>
        </child>
        <child>
          <object class="GtkTable" id="grid1">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="n_rows">3</property>
            <property name="n_columns">3</property>
            <child>
              <object class="GtkRadioButton" id="btnTopLeft">
                <property name="label" translatable="yes">TL</property>
                <property name="use_action_appearance">False</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">False</property>
                <property name="use_action_appearance">False</property>
                <property name="xalign">0.5</property>
                <property name="draw_indicator">False</property>
                <property name="group">btnHide</property>
              </object>
            </child>
            <child>
              <object class="GtkRadioButton" id="btnTopMiddle">
                <property name="label" translatable="yes">TM</property>
                <property name="use_action_appearance">False</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="receives_default">False</property>
                <property name="use_action_appearance">False</property>
                <property name="xalign">0.5</property>
                <property name="draw_indicator">False</property>
                <property name="group">btnHide</property>
              </object>
              <packing>
                <property name="left_attach">1</property>
              </packing>
